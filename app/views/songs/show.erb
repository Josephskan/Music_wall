<h1><%= @song.title %></h1>

<p><%= @song.author %></p>
<p>posted by <%= @song.user.username %>
<p><a href="<%= @song.url %>"><%= @song.url %></a></p>
<p>posted: <%= @song.created_at %></p>


<a href="/songs">Back to the songs listing</a>

<% if current_user %>
  <p>Welcome <%= current_user.username %>!</p>
  <p>Finished? <a href="/login">logout</a>!</p>
<% else %>
  <p><a href="/users/login">login</a> to add!</p>
<% end %>